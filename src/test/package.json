{
  "name": "@knowledge-curator/test-suite",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test:unit": "jest --testPathPattern=unit --maxWorkers=4",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:e2e": "jest --testPathPattern=e2e --config=jest.e2e.config.ts --maxWorkers=2",
    "test:api": "jest --testPathPattern=api --config=jest.api.config.ts",
    "test:web": "jest --testPathPattern=web --config=jest.web.config.ts",
    "test:python": "pytest --asyncio-mode=auto",
    "test:load": "k6 run load/k6/*.js --out json=results.json",
    "test:performance": "jest --testPathPattern=performance --config=jest.perf.config.ts",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov,html",
    "test:watch": "jest --watch --onlyChanged",
    "lint": "eslint . --ext .ts,.tsx --cache",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit --incremental"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.9.x",
    "aiohttp": "3.8.0",
    "axios": "1.6.x",
    "dayjs": "^1.11.0",
    "dotenv": "16.x",
    "grpcio-tools": "1.54.0",
    "httpx": "^0.24.0",
    "ioredis": "^5.0.0",
    "neo4j": "^5.0.0",
    "prom-client": "14.x",
    "requests": "2.31.0",
    "uuid": "^9.0.0",
    "winston": "3.x"
  },
  "devDependencies": {
    "@airbnb/node-memwatch": "2.x",
    "@faker-js/faker": "^8.0.0",
    "@jest/benchmark": "29.x",
    "@jest/globals": "29.x",
    "@jest/types": "^29.7.0",
    "@playwright/test": "^1.39.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.6",
    "axe-puppeteer": "1.x",
    "expect-puppeteer": "9.x",
    "http-status-codes": "2.3.x",
    "jest": "^29.7.0",
    "k6": "^0.47.0",
    "locust": "2.15.0",
    "mongomock": "^4.1.2",
    "opentelemetry-api": "1.19.0",
    "prometheus-client": "0.17.1",
    "puppeteer": "19.x",
    "pytest": "^7.4.0",
    "pytest-asyncio": "^0.21.0",
    "pytest-benchmark": "4.0.0",
    "pytest-cov": "4.1.0",
    "pytest-mock": "3.10.0",
    "pytest-timeout": "2.1.0",
    "pytest-xdist": "3.3.1",
    "redis-mock": "^0.1.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}